<h1>書籍一覧</h1>
<table class="table container mt-5">
  <thead>
    <tr>
      <th scope="col">id</th>
      <th scope="col">書籍名</th>
      <th scope="col">ISBN</th>
    </tr>
  </thead>
  <tbody>
   <% @book_list.each do |item| %>
